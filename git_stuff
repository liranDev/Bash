# this is git config

git config --global user.name = "liran shani"
git config --global user.email = "shaniliran@gmail.com"

git config --global core.editor vim
git config --global help.autocorrect 1
git config --global color.ui auto
git config --global core.autocrlf true

# some git commands

git checkout file --> revert added changes
git --oneline --graph
git branch --set-upstream master origin/master # tracks remote branch
